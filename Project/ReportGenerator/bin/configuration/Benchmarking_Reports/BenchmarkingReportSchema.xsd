<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:element name="TemplateConfiguration">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Template">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Filename">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:minLength value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Sheet">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Number">
                                <xs:simpleType>
                                    <xs:restriction base="xs:int">
                                        <xs:minInclusive value="0"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Broker">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Name" type="ExcelType"/>
                            <xs:element name="Price" type="ExcelType_Column"/>
                            <xs:element name="Supplier" type="ExcelType_Column"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="ConversionRate" type="ExcelType"/>
                <xs:element name="ConsultingDate" type="ExcelType"/>
                <xs:element name="Hour" type="ExcelType"/>
                <xs:element name="PickUpDate" type="ExcelType"/>
                <xs:element name="NumberOfDays" type="ExcelType_Column"/>
                <xs:element name="Locations">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="unbounded" name="Location">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="Cell" type="ExcelType"/>
                                        <xs:element name="Groups">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element maxOccurs="unbounded" name="Group">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="Name" type="xs:string"/>
                                                                <xs:element name="Row" type="RowType"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="type">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="Regular"/>
                                                <xs:enumeration value="Low"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="number">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:int">
                                                <xs:minInclusive value="1"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:complexType name="ExcelType">
        <xs:sequence>
            <xs:element name="Column" type="ColumnType"/>
            <xs:element name="Row" type="RowType"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="ExcelType_Column">
        <xs:sequence>
            <xs:element name="Column" type="ColumnType"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:simpleType name="ColumnType">
        <xs:restriction base="xs:string">
            <xs:pattern value="([A-Z])+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RowType">
        <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>
